        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Translucent">
        <activity android:name=".MainActivity"/>
        <activity android:name=".tela_cadastrocliente"/>
        <activity android:name=".tela_cadastrocontrato"/>
